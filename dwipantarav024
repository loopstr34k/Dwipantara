clear
echo -e "$blue Version Tool is 0.2.4 $white"
echo -e "$yellow Tool Under Maintenance :)"
echo " Reload Tool on 22:00"
echo " Thanks"
echo " Command :
                 reload      : Reload Script"
}
contact() {
clear
echo " "
echo -e $red      "         [ Contact Author ]"
