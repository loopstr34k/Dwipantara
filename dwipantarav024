echo -e $lightgreen     "     Instagram  ▶    loopstr34k $white"
echo ""
echo -e "$yellow Tool Under Maintenance :)"
echo " Reload Tool on 22:00"
echo " Thanks"
}
function install(){
clear
echo -e "$yellow Tool Under Maintenance :)"
echo " Reload Tool on 22:00"
echo " Thanks"
echo " Command :
                 reload      : Reload Script"

}
function argument(){
read -p " Dwipantara : > " dwipantara;
for i in $dwipantara;do
     $i
done;
}
while :
do
argument $dwipantara
done
